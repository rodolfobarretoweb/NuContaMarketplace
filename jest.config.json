{
  "preset": "react-native",
  "verbose": true,
  "globals": {
    "__DEV__": true,
    "NODE_ENV": "test"
  },
  "testPathIgnorePatterns": [
    "/utils/",
    "/node_modules/"
  ],
  "setupFiles": [
    "./__mocks__/index.js"
  ],
  "snapshotSerializers": [
    "./node_modules/jest-serializer-enzyme"
  ],
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "constants*",
    "/langs/",
    "/configs/",
    "/apis/",
    "/app/containers/index.js",
    "style.js"
  ]
}
